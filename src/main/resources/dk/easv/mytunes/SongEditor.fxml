                     <font>
                        <Font size="18.0" />
                     </font>
                  </Text>
                  <TextField fx:id="txtArtist" alignment="CENTER" prefHeight="26.0" prefWidth="250.0">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="38.0" prefWidth="600.0">
