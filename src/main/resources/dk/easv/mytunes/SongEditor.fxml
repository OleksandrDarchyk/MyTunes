                  </Button>
                  <Button mnemonicParsing="false" onAction="#onSaveClick" prefHeight="30.0" prefWidth="62.0" text="Save">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="60.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin></HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
